<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BlankFragment3">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:background="#FF6813"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


    </androidx.constraintlayout.widget.ConstraintLayout>


    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/celebrityImage"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:elevation="20dp"
        android:src="@drawable/img_7"
        app:civ_border_color="#FF6813"
        app:civ_border_width="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.029" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"

        android:src="@drawable/ic_keyboard_arrow_left_black_24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/semibold"
        android:text="Personal Info"
        android:layout_marginStart="7dp"
        android:layout_marginTop="5dp"
        android:textColor="#091020"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/celebrityImage" />

    <TextView
        android:id="@+id/textViewcchange"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:fontFamily="@font/semibold"
        android:text="change"
        android:textColor="#FF6813"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView11" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView2"
        android:layout_width="398dp"
        android:layout_height="190dp"
        android:layout_marginTop="10dp"
        app:cardCornerRadius="8dp"
        app:cardElevation="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="7dp"
                android:layout_marginTop="12dp"
                android:text="Your name"
                android:textColor="#272626"
                android:textSize="22sp"
                android:textStyle="normal"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <TextView
                android:id="@+id/textView21"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="7dp"
                android:text="Tommy Jason"
                android:textColor="@color/black"

                android:textSize="22sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/textView20"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textView20"
                app:layout_constraintVertical_bias="0.0" />

            <View
                android:id="@+id/view"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="2dp"
                android:background="@color/black"
                app:layout_constraintBottom_toTopOf="@+id/textViewss"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView20" />


            <TextView
                android:id="@+id/textViewUsername1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Hala zon"
                android:textColor="#272626"
                android:textSize="22sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/textViewUsername2"
                app:layout_constraintEnd_toEndOf="@+id/textView21"
                app:layout_constraintTop_toTopOf="@+id/textViewUsername2"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/textViewUsername2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="Username"
                android:textColor="@color/black"

                android:textSize="22sp"
                app:layout_constraintStart_toStartOf="@+id/textViewss"
                app:layout_constraintTop_toBottomOf="@+id/textViewss" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/black"
                app:layout_constraintBottom_toTopOf="@+id/textViewUsername2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView22" />

            <TextView
                android:id="@+id/textViewss"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="Phone number"
                android:textColor="#272626"
                android:textSize="22sp"
                android:textStyle="normal"
                app:layout_constraintStart_toStartOf="@+id/textView20"
                app:layout_constraintTop_toBottomOf="@+id/textView20" />

            <TextView
                android:id="@+id/textView22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="+234 806 2856 543"
                android:textColor="@color/black"
                android:textSize="19sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/textViewss"
                app:layout_constraintEnd_toEndOf="@+id/textView21"
                app:layout_constraintTop_toTopOf="@+id/textViewss"
                app:layout_constraintVertical_bias="1.0" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/black"
                app:layout_constraintBottom_toTopOf="@+id/textViewssDeliveryAddresses1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textViewUsername2" />


            <TextView
                android:id="@+id/textViewssDeliveryAddresses1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="Delivery Addresses"
                android:textColor="#272626"
                android:textSize="22sp"
                android:textStyle="normal"
                app:layout_constraintStart_toStartOf="@+id/textViewUsername2"
                app:layout_constraintTop_toBottomOf="@+id/textViewUsername2" />

            <TextView
                android:id="@+id/textView22DeliveryAddresses2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Gaza"
                android:textColor="@color/black"
                android:textSize="19sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/textViewssDeliveryAddresses1"
                app:layout_constraintEnd_toEndOf="@+id/textView21"
                app:layout_constraintTop_toTopOf="@+id/textViewssDeliveryAddresses1" />


        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardViewm"
        android:layout_width="0dp"
        android:layout_height="180dp"
        android:layout_margin="7dp"
        app:cardCornerRadius="8dp"
        app:cardElevation="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView2">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageViewyorders"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/img_8"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textViewssMyorders"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:text="My orders"
                android:textColor="#272626"
                android:textSize="22sp"
                android:textStyle="normal"
                app:layout_constraintBottom_toBottomOf="@+id/imageViewyorders"
                app:layout_constraintStart_toEndOf="@+id/imageViewyorders"
                app:layout_constraintTop_toTopOf="@+id/imageViewyorders" />

            <ImageView
                android:id="@+id/Paymentmethods1"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/img_9"
                app:layout_constraintEnd_toEndOf="@+id/imageViewyorders"
                app:layout_constraintStart_toStartOf="@+id/imageViewyorders"
                app:layout_constraintTop_toBottomOf="@+id/imageViewyorders" />

            <TextView
                android:id="@+id/Paymentmethods2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Payment methods"
                android:textColor="#272626"
                android:textSize="22sp"
                android:textStyle="normal"
                app:layout_constraintBottom_toBottomOf="@+id/Paymentmethods1"
                app:layout_constraintStart_toStartOf="@+id/textViewssMyorders"
                app:layout_constraintTop_toTopOf="@+id/Paymentmethods1" />


            <TextView
                android:id="@+id/Support"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Support"
                android:textColor="#272626"
                android:textSize="22sp"
                android:textStyle="normal"
                app:layout_constraintBottom_toBottomOf="@+id/Logout1"
                app:layout_constraintStart_toStartOf="@+id/Paymentmethods2"
                app:layout_constraintTop_toTopOf="@+id/Logout1" />


            <ImageView
                android:id="@+id/ss"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/img_10"
                app:layout_constraintEnd_toEndOf="@+id/Logout1"
                app:layout_constraintStart_toStartOf="@+id/Logout1"
                app:layout_constraintTop_toBottomOf="@+id/Logout1" />

            <ImageView
                android:id="@+id/Logout1"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/img_11"
                app:layout_constraintEnd_toEndOf="@+id/Paymentmethods1"
                app:layout_constraintStart_toStartOf="@+id/Paymentmethods1"
                app:layout_constraintTop_toBottomOf="@+id/Paymentmethods1" />

            <TextView
                android:id="@+id/Logout2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Logout"
                android:textColor="#272626"
                android:textSize="22sp"
                android:textStyle="normal"
                app:layout_constraintBottom_toBottomOf="@+id/ss"
                app:layout_constraintStart_toStartOf="@+id/textViewssMyorders"
                app:layout_constraintTop_toTopOf="@+id/ss" />


        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>